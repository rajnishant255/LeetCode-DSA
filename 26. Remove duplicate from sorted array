class Solution {
    public int removeDuplicates(int[] nums) {
        Arrays.sort(nums);
        int j=0;
        for(int i=1; i<nums.length; i++)
        {
            if(nums[i] != nums[j])     // checks for the nums of i index!=nums of j index
            {
                j++;
                nums[j]=nums[i];
            }
        }
        return j+1;                 //here we should return an array +1 as we just replacing the element
    }
}
