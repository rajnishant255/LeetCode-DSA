class Solution {

    public String value(int c) {
        if (c == 1000) return "M";
        if (c == 900)  return "CM";
        if (c == 500)  return "D";
        if (c == 400)  return "CD";
        if (c == 100)  return "C";
        if (c == 90)   return "XC";
        if (c == 50)   return "L";
        if (c == 40)   return "XL";
        if (c == 10)   return "X";
        if (c == 9)    return "IX";
        if (c == 5)    return "V";
        if (c == 4)    return "IV";
        return "I";
    }

    public String intToRoman(int num) {   // renamed nums â†’ num
        int[] values = {1000,900,500,400,100,90,50,40,10,9,5,4,1};

        StringBuilder sb = new StringBuilder();

        for (int i = 0; i < values.length; i++) {   // values.length
            while (num >= values[i]) {
                sb.append(value(values[i]));
                num -= values[i];
            }
        }
        return sb.toString();
    }
}
